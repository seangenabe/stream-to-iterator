{
  "name": "stream-to-iterator",
  "version": "3.0.1",
  "description": "Convert a node.js stream into an iterator.",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc --project .",
    "test": "npm run build && node --harmony_async_iteration node_modules/ava/cli.js",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seangenabe/stream-to-iterator.git"
  },
  "keywords": [
    "iterator",
    "stream",
    "readable",
    "adapter",
    "es2015",
    "es6",
    "async",
    "promise"
  ],
  "author": "Sean Genabe <seangenabe@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seangenabe/stream-to-iterator/issues"
  },
  "homepage": "https://github.com/seangenabe/stream-to-iterator#readme",
  "engines": {
    "node": ">=8.9 <10"
  },
  "dependencies": {
    "p-defer": "1.0.0",
    "pump": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^9.6.35",
    "ava": "^1.0.0-beta.1",
    "into-stream": "^3.1.0",
    "typescript": "^2.9.2"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "sources": [
      "lib/**/*.js"
    ],
    "timeout": "10s",
    "babel": false,
    "compileEnhancements": false
  }
}
